<form action="/api/users/premuim/{{userId}}" method="post" enctype="multipart/form-data">
    <h2>Upload Files</h2>

    <label for="profileImage">Profile Image:</label>
    <input type="file" name="profiles" id="profileImage" accept="image/*"><br>

    <label for="productImage">Product Image:</label>
    <input type="file" name="products" id="productImage" accept="image/*"><br>

    <label for="document">Document:</label>
    <input type="file" name="document" id="document" accept=".pdf,.doc,.docx"><br>

    <button type="submit">Become premiun</button>
</form>

